void setup() {
  pinMode(8, OUTPUT);
}

void loop() {

//S
  digitalWrite(8, HIGH); 
  delay(200); 
  digitalWrite(8, LOW); 
  delay(200);
  digitalWrite(8, HIGH); 
  delay(200); 
  digitalWrite(8, LOW); 
  delay(200);
  digitalWrite(8, HIGH); 
  delay(200); 
  digitalWrite(8, LOW); 
  delay(600);

//O
  digitalWrite(8, HIGH); 
  delay(600); 
  digitalWrite(8, LOW); 
  delay(200);
  digitalWrite(8, HIGH); 
  delay(600); 
  digitalWrite(8, LOW); 
  delay(200);
  digitalWrite(8, HIGH); 
  delay(600); 
  digitalWrite(8, LOW); 
  delay(600);

//S
  digitalWrite(8, HIGH); 
  delay(200); 
  digitalWrite(8, LOW); 
  delay(200);
  digitalWrite(8, HIGH); 
  delay(200); 
  digitalWrite(8, LOW); 
  delay(200);
  digitalWrite(8, HIGH); 
  delay(200); 
  digitalWrite(8, LOW); 
  delay(1400);
}